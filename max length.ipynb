    "def FindMaxLength(lst): \n",
    "    maxList = max((x) for x in lst) \n",
    "    maxLength = max(len(x) for x in lst ) \n",
    "  \n",
    "    return maxList, maxLength \n",
    "      \n",
    "# Driver Code \n",
    "lst = [['A','B'], ['A', 'B' , 'c'], ['A', 'B', 'C'] , ['A','B','c']] \n",
    "print(FindMaxLength(lst))"
   ]
